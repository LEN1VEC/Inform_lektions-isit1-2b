    System.Math;

function m_compute(a, b, c: Real; var DiscrIsNotNull: Boolean): Real;
var
    d: Real;
begin
    d := Power(b, 2) - 4 * a * c;

    if (d >= 0) then
    DiscrIsNotNull := True;

end;

end.


